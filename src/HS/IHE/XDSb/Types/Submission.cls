Class HS.IHE.XDSb.Types.Submission Extends (ObjectRef, HS.Util.CopyFrom) [ Inheritance = right ]
{

Parameter XMLIGNORENULL = "INPUTONLY";

/// Authors of this submission set
Property Author As list Of HS.IHE.XDSb.Types.Author(XMLPROJECTION = "ELEMENT");

/// Submitted / Approved -- generated by registry
Property AvailabilityStatus As %String(VALUELIST = ",Submitted,Approved");

/// Free text description
Property Comments As %String(MAXLEN = "");

/// Indicates the overall type for all documents
Property ContentTypeCode As HS.IHE.XDSb.Types.CodedValue;

/// Optional list of recipient
Property IntendedRecipient As list Of HS.IHE.XDSb.Types.SlotValue(XMLITEMNAME = "Value");

/// Patient ID
Property PatientId As %String(CONTENT = "ESCAPE", MAXLEN = "");

/// Source OID
Property SourceId As %String(MAXLEN = "");

/// When submission was generated, UTC, HL7v2 format
Property SubmissionTime As %String(MAXLEN = "");

/// Free text title
Property Title As %String(MAXLEN = "");

/// OID assigned by the source
Property UniqueId As %String(MAXLEN = "");

/// Collection of slots from submission set
Property SubmissionSlots As list Of HS.IHE.XDSb.Types.Slot;

}
