<if condition='target.id.value &apos;= "internalDiagnosticOrder"' >
<true>
<if condition='source.Specimens.Count() &gt; 0' >
<true>
<foreach property='source.Specimens()' key='k1' >
<subtransform class='HS.FHIR.DTL.FromSDA.SubTransform.Specimen' targetObj='target.contained.(iContained).Specimen' sourceObj='source.Specimens.(k1)' aux='.aux' />
<assign value='##class(HS.FHIR.DTL.Utils).CreateUUID()' property='target.contained.(iContained).Specimen.id.value' action='set' />
<assign value='"#"_target.contained.(iContained).Specimen.id.value' property='target.specimen.(k1).reference.value' action='set' />
